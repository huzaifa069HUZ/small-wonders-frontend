{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/contact",
            "dest": "/api/contact.js"
        },
        {
            "src": "/api/admission",
            "dest": "/api/admission.js"
        },
        {
            "src": "/api/hello",
            "dest": "/api/hello.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}